{
	"name": "Easynews Searcher",
	"version": "0.0.2",
	"description": "Browser extension for automatic NZB file download from the Easynews Web Search.",
	"homepage_url": "https://github.com/Tensai75/EasynewsSearcher",
	"manifest_version": 3,
	"minimum_chrome_version": "100",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage",
		"contextMenus",
		"downloads",
		"notifications"
	],
	"host_permissions": [
		"https://members.easynews.com/*"
	],
	"options_ui": {
		"browser_style": true,
		"page": "options.html"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	}
}
